  "scripts": {
    "db": "rm -rf ./prisma/oauth2.db && npx prisma generate && npx prisma db push",
    "dev": "rm -rf ./.next && next dev -p 6969",
    "build": "rm -rf ./.next && next build",
    "compile": "tsc --jsx react-jsx --project tsconfig.compile.json",
    "start": "next start -p 6969",
