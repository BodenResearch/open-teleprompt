    "fix": "prettier --write . && next lint --fix",
    "storybook": "storybook dev -p 6069",
    "storybook-build": "storybook build",
    "test-storybook": "test-storybook --url http://localhost:6069",
    "storybook-deploy": "storybook build && gh-pages -d storybook-static"
  },
